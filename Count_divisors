import java.io.*;
import java.util.Scanner;
class TestClass {
    public static void main(String args[] ) throws Exception {
     Scanner sc=new Scanner(System.in);
     int l=sc.nextInt();
     int r=sc.nextInt();
     int k=sc.nextInt();
     int d;
     d=0;
     for(int i=l;i<=r;i++)
     {
       if(i%k==0)
       {
         d=d+1;
       }
     }
     System.out.println(d);
     
    }
}
